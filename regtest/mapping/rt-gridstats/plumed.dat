READ VALUES=c1.* FILE=colv_in LABEL=c1

SMAP-FIELD ...
  TYPE=EUCLIDEAN
  HIGH_DIM_FUNCTION={SMAP R_0=2 A=3 B=9}
  LOW_DIM_FUNCTION={SMAP R_0=2 A=2 B=2}
  STRESS_GRID={MIN=-10,-10 MAX=10,10 NBIN=25,25}
  REFERENCE=bespoke.pdb
  PROPERTY=smap1,smap2
  LABEL=smap
... SMAP-FIELD

GRIDSTATS ...
  FUNC=smap
  SIGMA=0.01
  NGRIDPOINTS=250,250
  LABEL=stat
... GRIDSTATS

FINDPROJECTION ...
  FUNC=smap
  LABEL=s1
... FINDPROJECTION

PRINT ARG=stat.*,s1.* FMT=%8.4f FILE=colvar
